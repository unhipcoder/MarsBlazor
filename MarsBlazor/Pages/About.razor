@page "/about"

    <div class="inner fadeInRight">

        <h1>About MarsBlazor</h1>

        <p>Current count: @currentCount</p>


        <button class="btn btn-primary" @onclick="@IncrementCount">Click me</button>

    </div>


@code {

    int currentCount = 0;

    void IncrementCount()
    {
        currentCount++;
    }
}
